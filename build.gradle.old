apply plugin: 'java'
apply plugin: 'maven'

group = 'com.github.magneticflux-'

description = """"""

sourceCompatibility = 1.8
targetCompatibility = 1.8

repositories {

    maven { url "http://maven.jzy3d.org/snapshots" }
    maven { url "http://maven.jzy3d.org/releases" }
    maven { url "http://repo.maven.apache.org/maven2" }
    maven { url "https://jitpack.io" }
}

dependencies {
    compile group: 'org.jzy3d', name: 'jzy3d-api', version: '0.9.1'
    compile group: 'org.jfree', name: 'jfreechart', version: '1.0.19'
    compile group: 'org.apache.commons', name: 'commons-math3', version: '3.6'
    compile group: 'org.apache.commons', name: 'commons-collections4', version: '4.0'
    compile group: 'org.apache.commons', name: 'commons-lang3', version: '3.4'
    compile group: 'com.miglayout', name: 'miglayout-swing', version: '5.0'
    compile 'com.github.magneticflux-:JNSGA-II:-SNAPSHOT'
}
